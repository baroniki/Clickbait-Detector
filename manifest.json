{
    "manifest_version": 2,
    "name": "Clickbait Detector",
    "version": "1.0",
    "background": 
    {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": 
        {
            "16": "images/bait16.png",
            "32": "images/bait32.png",
            "48": "images/bait48.png",
            "128": "images/bait128.png"
        }
    },
    "icons": 
    {
        "16": "images/bait16.png",
        "32": "images/bait32.png",
        "48": "images/bait48.png",
        "128": "images/bait128.png"
    }
}